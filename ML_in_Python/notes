s = 'hello my name is Sam'
s.(tab)
#look up method information for object s
-----------
x = [(1,2), (3,4), (5,6)]
for (a,b) in x:
    print(b)
#tuple unpacking: output: 2, 4, 6
------------
list = [1,2,3]
list.pop()
#out: list = [1,2]
------------
list = [1, 2, 3]
list.pop(0)
#out: list = [2,3]