seq = [1,2,3,4,5]
def times2(var):
    return var*2

list(map(times,seq))
#map means apply times to all members in seq
-----------
t = lambda var:var*2
t(2) #return 12
list(map(lambda var:var*2, seq))
#lambda expression
-----------
list[filter(lambda num: num%2 == 0, seq)]
#out: [2,4]
